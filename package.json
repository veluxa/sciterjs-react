{
  "name": "sciter-react-template",
  "version": "1.0.0",
  "description": "sciter-react",
  "git repository": "https://github.com/veluxa/sciterjs-react",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.conf.js --hot --inline",
    "buildwww": "webpack --config webpack.prod.conf.js",
    "scapp": "%cd%/bin/windows/scapp.exe ./dist/index.html",
    "macScapp": "./bin/macosx/scapp.exe ./dist/index.html",
    "buildscapp": "SET PLATFORM=sciter && webpack --config webpack.prod.conf.js && npm run scapp",
    "buildaardio": "SET PLATFORM=sciter && webpack --config webpack.aardio.prod.conf.js",
    "inspector": "%cd%/bin/windows/inspector.exe"
  },
  "keywords": [
    "sciterjs"
  ],
  "author": "veluxa",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "less": "^4.0.0",
    "less-loader": "^7.2.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "precss": "^4.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "sciterjs-react": "^1.0.3"
  }
}
